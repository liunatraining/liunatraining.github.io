  {
    "_id": "5970cedf87aa48a40dea67b9",
    "_layout": "full",
    "_type": "component",
    "_parentId": "5970cedc87aa48a40dea67b8",
    "body": "<p>Unwelcome conduct that is based on one or more of the following:</p>",
    "displayTitle": "Once you have finished with this section, scroll down for more",
    "title": "Component title",
    "_isAvailable": true,
    "_isOptional": false,
    "_classes": "",
    "_component": "expose",
    "_pageLevelProgress": {
      "_isEnabled": true
    },
    "instruction": "Click on a topic for more detail.",
    "_columns": 3,
    "_animationType": "fade",
    "_items": [
      {
        "front": "Race",
        "back": {
          "title": "",
          "body": "<p>&bull;Person is of a certain race</p><p>&bull;Personal characteristics associated with race (such as hair texture, skin color, or certain facial features)</p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Color",
        "back": {
          "title": "",
          "body": "<p>&bull;Skin color complexion</p><p>&bull;Married to (or associated with) a person of a certain race or color</p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "National Origin",
        "back": {
          "title": "",
          "body": "<p>&bull;From a particular country or part of the world</p><p>&bull;ethnicity or accent</p><p>&bull;appear to be of a certain ethnic background (even if they are not)</p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      }
    ]
  },
  {
    "_id": "5970dc9687aa48a40dea67bc",
    "_layout": "full",
