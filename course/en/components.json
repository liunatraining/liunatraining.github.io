    "_isOptional": false,
    "_classes": "",
    "_component": "expose",
    "_pageLevelProgress": {
      "_isEnabled": true
    },
    "instruction": "Click the boxes to reveal more information about the different ways in which people can be harassed.",
    "_columns": 4,
    "_animationType": "flipUp",
    "_items": [
      {
        "front": "Mentally",
        "back": {
          "title": "",
          "body": "<p>Leave lasting emotional damage</p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Physically",
        "back": {
          "title": "",
          "body": "<p>Stress and physical abuse can be a result</p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Economically",
        "back": {
          "title": "",
          "body": "<p>It can cost both the harasser and the employer large amounts of money</p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      },
      {
        "front": "Reputation",
        "back": {
          "title": "",
          "body": "<p>Reputations can be ruined, losing work and destroying careers</p>",
          "_graphic": {
            "alt": "",
            "src": ""
          }
        }
      }
    ]
